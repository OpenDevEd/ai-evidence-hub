{% macro toggleOff(class_) %}
<svg class="{{ class_ }}" width="32" height="16" viewBox="0 0 32 16" fill="none" xmlns="http://www.w3.org/2000/svg">
    <rect x="0.25" y="0.25" width="31.5" height="15.5" rx="7.75" fill="black" fill-opacity="0.2" stroke="#A4A4A5"
        stroke-width="0.5" />
    <circle cx="8" cy="8" r="6" fill="white" fill-opacity="0.8" />
</svg>
{% endmacro %}

{% macro toggleOn(class_) %}
<svg class="{{ class_ }}" width="32" height="16" viewBox="0 0 32 16" fill="none" xmlns="http://www.w3.org/2000/svg">
    <rect x="0.25" y="0.25" width="31.5" height="15.5" rx="7.75" fill="#5CACFD" stroke="#A4A4A5" stroke-width="0.5" />
    <circle cx="24" cy="8" r="6" fill="white" fill-opacity="0.8" />
</svg>
{% endmacro %}

{% macro checkSquare(class_, width, height) %}
<svg class="{{ class_ }}" width="{{ width }}" height="{{ height }}" viewBox="0 0 24 25" fill="none"
    xmlns="http://www.w3.org/2000/svg">
    <rect x="0.125" y="0.145508" width="23.75" height="23.75" rx="5.875" fill="#E68124" stroke="#712916"
        stroke-width="0.25" />
    <path d="M7 12.6491L9.69391 15.7348L16.4286 8.02051" stroke="white" stroke-width="2" stroke-linecap="round"
        stroke-linejoin="round" />
</svg>
{% endmacro %}

{% macro noCheckSquare(class_, width, height) %}
<svg class="{{ class_ }}" width="{{ width }}" height="{{ height }}" viewBox="0 0 28 28" fill="none"
    xmlns="http://www.w3.org/2000/svg">
    <rect x="1" y="1" width="26" height="26" rx="5" {% if theme=='dark' %} stroke="#4B5563" {% else %} stroke="#D1D5DB"
        {% endif %} stroke-opacity="1" stroke-width="2" />
</svg>
{% endmacro %}

{% macro expandIcon(class_, width, height) %}
